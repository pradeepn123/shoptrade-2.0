        "da24a044-9580-4c39-b9ab-4cbbd4d4875d": {
          "type": "Image_blocks",
          "settings": {
            "show_contactpopUP": true,
            "title": "Shopify App Solutions",
            "box_url": "",
            "gib_para": "<p>From public apps designed for the Shopify marketplace to custom solutions built just for you, we do it all. We excel in designing and developing apps that automate business processes while elevating the user experience.<\/p>",
            "enble_video": false,
