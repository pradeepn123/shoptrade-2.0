    "about-mision": {
      "type": "about-mision",
      "blocks": {
        "2f4482e8-d2e5-40d3-8d68-3337696b5cf3": {
          "type": "mission",
          "settings": {
            "heading": "Our Mission",
            "subheading": "Our mission lays beyond pixel perfect design and high quality code, which are just tools on our path to establishing a network of entrepreneurs that help each other with the sole purpose of scaling their business. In an ever changing online world, the only way to stay ahead of the curve is by surrounding yourself with like minded energetic professionals."
          }
        },
        "9b6d8825-12d4-4c22-916f-288410252c45": {
          "type": "mission",
          "settings": {
            "heading": "Our Vision",
            "subheading": "Our vision is to become the swiss knife that every online entrepreneur has in their pockets and we’re getting there through smart partnerships with established companies, building our own suite of products and enabling clients to reach new business heights by providing them on-demand teams of professionals."
          }
        }
      },
