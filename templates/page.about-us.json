    "about-value": {
      "type": "about-value",
      "blocks": {
        "6fa82323-d7a3-4a2b-958a-46837237aba7": {
          "type": "vedioCard",
          "settings": {
            "vedioHead": "Personal Touch",
            "vedioSubtext": "Each of our customers ranks first on our priority list. We’re constantly seeking ways to make your life easier. We treat each project as our own and we get heavily invested in your product, no matter what."
          }
        },
        "73c5c6de-80e1-4bc5-b8ba-f3e7b55d5115": {
          "type": "vedioCard",
          "settings": {
            "vedioHead": "Transparency",
            "vedioSubtext": "We believe that transparency is the foundation of successful long term collaborations, and we love those. When working with ShopTrade you’ll embark in a journey while our team of professionals will guide you at every step."
          }
        },
        "e3f355ec-e052-419e-9be4-872532236f38": {
          "type": "vedioCard",
          "settings": {
            "vedioHead": "Passion",
            "vedioSubtext": "We surround ourselves only with passionate people, because we believe in win-win-win relationships where our clients are delighted with successful products that help their user base and we thrive while doing so."
          }
        },
        "4ececf78-c611-4a13-b56a-1756af125f0d": {
          "type": "vedioCard",
          "settings": {
            "vedioHead": "Quality",
            "vedioSubtext": "Our priority is delivering highly scalable pixel perfect solutions. We’re not here to jump from one project to the other but rather to help our clients becoming established brands in their market, and the only way to do so is with quality services."
          }
        }
      },
